
public class DoMath {
	public static void main(String[] args){
		System.out.println(Calculate.cube(2));
		System.out.println(Calculate.average(3, 4));
		System.out.println(Calculate.average(2, 4, 6));
		System.out.println(Calculate.toDegrees(30));
		System.out.println(Calculate.toRadians(6));
		System.out.println(Calculate.discriminant(0, 0, 0));
		System.out.println(Calculate.toImproperFrac(3,1,2));
		System.out.println(Calculate.MixedNum(3,2));
		System.out.println(Calculate.foil(10, 4, 3, "n"));
		System.out.println(Calculate.isDivisibleBy(4, 86));
		System.out.println(Calculate.absValue(88));
		System.out.println(Calculate.max(34,45));
		System.out.println(Calculate.max(34,45,67));
		System.out.println(Calculate.min(34,78));
		System.out.println(Calculate.round2(5));
		System.out.println(Calculate.exponent(5, 0));
		System.out.println(Calculate.factorial(5));
		System.out.println(Calculate.isPrime(5));
		System.out.println(Calculate.gcf(5, 0));
		System.out.println(Calculate.sqrt(5));
		System.out.println(Calculate.quadForm(5, 0, 0));
	}
}
