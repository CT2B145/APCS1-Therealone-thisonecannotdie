public class ProcessingNumbers {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		Scanner num = new Scanner(System.in);
		int next = num.nextInt();
		int max;
		int min;
		
	for(int i = 1;i<=16 ;i++){
		
		if(next>max){
			max = next;
			System.out.println(max);
			}
		if(min<next){
			min = next;
			System.out.println(min);
			
		if(next%2 == 0)
			System.out
		}

	}

	}
}
